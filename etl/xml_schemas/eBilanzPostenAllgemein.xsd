	</xs:element>
	<xs:element name="ANHANG" type="xs:string"/>
	<xs:complexType name="SPIEGEL">
		<xs:sequence>
			<xs:element name="SPALTE_1" type="negativepositiveBetrag" minOccurs="0"/>
			<xs:element name="SPALTE_2" type="negativepositiveBetrag" minOccurs="0"/>
			<xs:element name="SPALTE_3" type="negativepositiveBetrag" minOccurs="0"/>
