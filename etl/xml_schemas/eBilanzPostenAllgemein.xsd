	<xs:complexType name="POSTEN_SPIEGEL_SUB_FREI">
		<xs:sequence>
			<xs:element name="BETRAG" type="negativepositiveBetrag"/>
			<xs:element name="BETRAG_VJ" type="negativepositiveBetrag" minOccurs="0"/>
			<xs:element ref="TEXT"/>
			<xs:element ref="ANHANG" minOccurs="0"/>
			<xs:element name="SPIEGELWERTE" type="SPIEGEL" minOccurs="0"/>
			<xs:element name="FREIER_SUB_POSTEN" type="POSTEN_SPIEGEL_FREI" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	
	<!-- Ein (nicht freier) Posten, mit positiven und negativen Beträgen - mit Spiegel und SubPosten(positive und negative Beträge) -->
	<xs:complexType name="POSTEN_SPIEGEL_SUB">
		<xs:sequence>
			<xs:element name="BETRAG" type="negativepositiveBetrag"/>
			<xs:element name="BETRAG_VJ" type="negativepositiveBetrag" minOccurs="0"/>
			<xs:element ref="TEXT" minOccurs="0"/>
			<xs:element ref="ANHANG" minOccurs="0"/>
			<xs:element name="SPIEGELWERTE" type="SPIEGEL" minOccurs="0"/>
			<xs:element name="FREIER_SUB_POSTEN" type="POSTEN_SPIEGEL_FREI" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
