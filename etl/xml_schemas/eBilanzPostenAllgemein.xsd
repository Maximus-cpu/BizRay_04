	<xs:complexType name="POSTEN_FREI_SUB">
		<xs:sequence>
			<xs:element name="BETRAG" type="negativepositiveBetrag"/>
			<xs:element name="BETRAG_VJ" type="negativepositiveBetrag" minOccurs="0"/>
			<xs:element ref="TEXT"/>
			<xs:element ref="ANHANG" minOccurs="0"/>
			<xs:element name="FREIER_SUB_POSTEN" type="POSTEN_FREI" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	
