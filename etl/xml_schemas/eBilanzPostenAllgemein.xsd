	<!--Beginn globale Typen Definitionen Allgemeine Daten JUSTIZ -->
	<xs:simpleType name="betrag">
		<xs:restriction base="xs:string">
			<xs:pattern value="-?[0-9]{1,12}(\.[0-9]{1,2})?"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- Zeichensatz fuer Bezug -->
	<xs:simpleType name="zsBezug">
		<xs:restriction base="xs:string">
			<xs:maxLength value="25"/>
