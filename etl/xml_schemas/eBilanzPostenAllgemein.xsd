		</xs:sequence>
	</xs:complexType>
	
	<!-- Freier Posten (Element TEXT ist daher mandatory) mit positiven und negativen Beträgen - ohne Spiegel und ohne SubPosten -->
	<xs:complexType name="POSTEN_FREI">
		<xs:sequence>
			<xs:element name="BETRAG" type="negativepositiveBetrag"/>
			<xs:element name="BETRAG_VJ" type="negativepositiveBetrag" minOccurs="0"/>
			<xs:element ref="TEXT"/>
			<xs:element ref="ANHANG" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	
	<!-- Ein (nicht freier) Posten, mit negativen und positiven Beträgen - ohne Spiegel und ohne SubPosten -->
	<xs:complexType name="POSTEN">
		<xs:sequence>
			<xs:element name="BETRAG" type="negativepositiveBetrag"/>
			<xs:element name="BETRAG_VJ" type="negativepositiveBetrag" minOccurs="0"/>
			<xs:element ref="TEXT" minOccurs="0"/>
			<xs:element ref="ANHANG" minOccurs="0"/>
